{
  "name": "spryker/cms-content-widget-cms-block-connector",
  "type": "library",
  "description": "CmsContentWidgetCmsBlockConnector module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/cms-block-storage": "^1.1.0 || ^2.0.0",
    "spryker/cms-content-widget": "^1.0.0",
    "spryker/kernel": "^3.52.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/twig": "*"
  },
  "suggest": {
    "spryker/twig": "If you want to use CmsBlockContentWidgetPlugin."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
